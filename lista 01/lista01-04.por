programa {
	funcao inicio() {

		esvreva("Produto SImples")
		escreva("Leia dois valores inteiros.\n")
		escreva("A seguir, calcule o produto entre estes dois valores e atribua esta operação à variável prod.\n")
		escreva("A seguir mostre a variável prod com mensagem correspondente.\n")
		
		inteiro a, b, prod

    escreva("Digite o valor de A: ")
    leia(a)
    escreva("Digite o valor de B: ")
    leia(b)
    prod = a * b
    escreva("PROD = ", prod)

	}
}
