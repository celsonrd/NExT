programa {
	funcao inicio() {
		escreva("Positivo ou Negativo\n")
        escreva("Faça um programa que leia um número e exibe na tela se o valor é positivo ou negativo.\n")
        
        inteiro num
        
        escreva("Digite um número: ")
        leia(num)
        
        se (num > 0){
            
            escreva("Número positivo")
            
        } senao {
            
            escreva("Número negativo")
            
        }
	}
}
